Context("Numeric");

#given
$m = random(.5,.7,.01);
$i = random(.1,.3,0.01);
$l = random(.7,1.1,.01);

#computation 
$i2 = $i + $m * $l * $l;

#set tolerance
Context()->flags->set(
tolerance=>.01);

