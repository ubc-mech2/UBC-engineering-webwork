DOCUMENT();

#image scale
$imgScale = .5;

#image aspect ratio
$width = $imgScale*588;
$height = $imgScale*346;

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
	"parserRadioButtons.pl",
