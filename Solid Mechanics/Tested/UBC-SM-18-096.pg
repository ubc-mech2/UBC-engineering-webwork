Context("Numeric");
Context()->variables->add(A=>"Real");
Context()->variables->add(Y=>"Real");
Context()->variables->add(t=>"Real");
Context()->variables->remove("x");
#given


#computation 
$Ans = Formula("2 Y A sin(t)");

#set tolerance
Context()->flags->set(
tolerance=>.01);

